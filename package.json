{
  "name": "signalwire",
  "version": "1.0.2",
  "description": "Client library for connecting to SignalWire.",
  "author": "SignalWire Team <open.source@signalwire.com>",
  "main": "dist/cjs/node/index.js",
  "module": "dist/esm/web/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean-build": "rm -rf dist/* && npm run build",
    "build": "npm run build-es5 && npm run build-es6 && npm run build-node",
    "build-es5": "cd packages/web && webpack -p",
    "start:dev": "webpack-dev-server --host 0.0.0.0 --mode development",
    "build-es6": "cd packages/web && tsc -p tsconfig.es6.json",
    "watch-es6": "cd packages/web && tsc -p tsconfig.es6.json -w",
    "build-node": "cd packages/node && tsc",
    "watch-node": "cd packages/node && tsc -w",
    "tslint": "npm run tslint:web && npm run tslint:node",
    "tslint:web": "tslint -p packages/web/tsconfig.es6.json",
    "tslint:node": "tslint -p packages/node/tsconfig.json",
    "test:web": "cd packages/web && jest --forceExit --coverage --detectOpenHandles",
    "test:node": "cd packages/node && jest --forceExit --coverage --detectOpenHandles",
    "watch:test:web": "npm run test:web -- --watchAll",
    "watch:test:node": "npm run test:node -- --watchAll"
  },
  "keywords": [
    "signalwire",
    "laml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signalwire/signalwire-node"
  },
  "bugs": {
    "url": "https://github.com/signalwire/signalwire-node/issues"
  },
  "homepage": "https://github.com/signalwire/signalwire-node",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "faye-websocket": "^0.11.1",
    "twilio": "^3.26.1",
    "uuid": "^3.3.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/jest": "^23.3.11",
    "@types/node": "^9.4.6",
    "@types/uuid": "^3.4.3",
    "typescript": "^3.2.4",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "mockttp": "^0.12.6",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0"
  }
}

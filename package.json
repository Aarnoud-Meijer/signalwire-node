{
  "name": "signalwire-client-js",
  "version": "0.0.1",
  "description": "Client library for connecting to the SignalWire system",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Edoardo Gallo <egallo@signalwire.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run build-es5 && npm run build-es6",
    "build-es5": "webpack -p",
    "build-es6": "tsc -p tsconfig.es6.json",
    "start:dev": "webpack-dev-server --mode development",
    "tslint": "npm run tslint-es5 && npm run tslint-es6",
    "tslint-es5": "tslint -c tslint.json -p tsconfig.es5.json",
    "tslint-es6": "tslint -c tslint.json -p tsconfig.es6.json",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "debug": "npm run build && npm run watch-debug"
  },
  "dependencies": {
    "@types/uuid": "^3.4.3",
    "errorhandler": "^1.5.0",
    "lodash": "^4.17.10",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/errorhandler": "^0.0.32",
    "@types/jest": "^22.1.3",
    "@types/lodash": "^4.14.91",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "@types/supertest": "^2.0.4",
    "@types/webrtc": "0.0.23",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "jest": "^22.0.4",
    "nodemon": "^1.18.0",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10"
  }
}

{
  "name": "signalwire-client-js",
  "version": "0.0.1",
  "description": "Client library for connecting to the SignalWire system",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Edoardo Gallo <egallo@signalwire.com>",
  "main": "dist/es5/index.js",
  "module": "dist/es6/index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run build-es5 && npm run build-es6",
    "build-es5": "webpack -p",
    "build-es6": "tsc -p tsconfig.es6.json",
    "watch-es6": "tsc -p tsconfig.es6.json -w",
    "start:dev": "webpack-dev-server --mode development",
    "tslint": "npm run tslint-es5 && npm run tslint-es6",
    "tslint-es5": "tslint -c tslint.json -p tsconfig.es5.json",
    "tslint-es6": "tslint -c tslint.json -p tsconfig.es6.json",
    "test": "jest --forceExit --coverage --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "debug": "npm run build && npm run watch-debug"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/uuid": "^3.4.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.9.1",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10"
  }
}
